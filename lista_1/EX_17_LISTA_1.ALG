Algoritmo "Ex_17_Lista_1"

Var
   n,no,nom,nome: caracter
   i,id,ida,idad: inteiro
   media: real
   
Inicio
      escreval("Informe 4 nomes:")
         leia(n,no,nom,nome)
      escreval("Agora digite a idade respectivamente:")
         leia(i,id,ida,idad)
         media <- ((i+id+ida+idad)/4)
      escreval("A media da idade dessas pesoas e:", media)


Fimalgoritmo
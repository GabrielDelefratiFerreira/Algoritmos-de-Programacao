Algoritmo "Ex.1 14-08-2024"

Var
   salario,salarioN: real
   nome: caracter


Inicio
      escreval("Digite seu nome:")
      leia(nome)
      escreval("Digite seu salario:")
      leia(salario)
      salarioN<- salario * 1.285
      escreval(" Sr.(a) ", nome," seu novo salario e: ", salarioN)


Fimalgoritmo
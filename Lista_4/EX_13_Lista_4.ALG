Algoritmo "EX_13"

Var
   no,no2: caracter

Inicio
      escreval("Informe dois nomes:")
        leia(no,no2)
      se(no > no2) entao
              escreval(no," vem depois de ", no2," em ordem alfabetica")
      senao
              escreval(no2," vem depois de ", no," em ordem alfabetica")
      fimse

Fimalgoritmo